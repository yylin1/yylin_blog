  background-color: #00000000;
}

/* Fix jumping scrollbar issue */
/* :root {
  scrollbar-gutter: stable;
} */

html {
  overflow-y: scroll;
}

:root {
  /* 主题相关变量 */
  --hs-border-color: #1d4ed8;
  --hs-decoration-color: #1d4ed8;
  --hs-text-color-hover: #1d4ed8;
  --hs-text-color-hover-dark: #3b82f6;
  --hs-link-color: #1d4ed8;

  --hs-toc-active-border-color: #1d4ed8;
  --hs-toc-active-bg-color: #eff6ff;
  --hs-toc-active-bg-color-dark: rgb(31 41 55 / 85);
  --hs-toc-active-text-color: #1d4ed8;
  --hs-toc-active-text-color-dark: #3b82f6;

  --hs-scrollbar-color: rgb(156 163 175 / 40);
  --hs-scrollbar-color-hover: #1d4ed8;

  overflow-y: auto;
  overflow-x: hidden;
}

:root body {
  position: absolute;
}

body {
  width: 100vw;
}

.theme-toc {
  display: flex;
  border-inline-start-width: 3px;
  --tw-border-opacity: 1;
  border-color: rgb(209 213 219 / var(--tw-border-opacity, 1));
  padding: 0.5rem;
  --tw-text-opacity: 1;
  color: rgb(107 114 128 / var(--tw-text-opacity, 1));
}

.theme-toc:hover {
  --tw-border-opacity: 1;
  border-color: rgb(75 85 99 / var(--tw-border-opacity, 1));
  --tw-bg-opacity: 1;
  background-color: rgb(249 250 251 / var(--tw-bg-opacity, 1));
  --tw-text-opacity: 1;
  color: rgb(55 65 81 / var(--tw-text-opacity, 1));
}

.theme-toc:is(.dark *) {
  --tw-border-opacity: 1;
  border-color: rgb(75 85 99 / var(--tw-border-opacity, 1));
  --tw-text-opacity: 1;
  color: rgb(107 114 128 / var(--tw-text-opacity, 1));
}

.theme-toc:hover:is(.dark *) {
  background-color: rgb(31 41 55 / 0.85);
  --tw-text-opacity: 1;
  color: rgb(209 213 219 / var(--tw-text-opacity, 1));
}

@media (min-width: 1024px) {

  .theme-toc {
    --tw-border-opacity: 1;
    border-color: rgb(243 244 246 / var(--tw-border-opacity, 1));
  }
}

.theme-toc-active {
  display: flex;
  border-inline-start-width: 3px;
  border-color: var(--hs-toc-active-border-color);
  background-color: var(--hs-toc-active-bg-color);
  padding: 0.5rem;
  color: var(--hs-toc-active-text-color);
}

.theme-toc-active:is(.dark *) {
  background-color: var(--hs-toc-active-bg-color-dark);
  color: var(--hs-toc-active-text-color-dark);
}

.theme-toc-nest-1 {
  padding-left: 1rem;
}

.theme-toc-nest-2 {
  padding-left: 1.25rem;
}

.theme-toc-nest-3 {
  padding-left: 1.5rem;
}

/* 圆形按钮特效 */
.buttom-circle {
  border-radius: 9999px;
  --tw-shadow: 0 2px 15px -3px rgba(0,0,0,0.07),0 10px 20px -2px rgba(0,0,0,0.04);
  --tw-shadow-colored: 0 2px 15px -3px var(--tw-shadow-color), 0 10px 20px -2px var(--tw-shadow-color);
  box-shadow: var(--tw-ring-offset-shadow, 0 0 #0000), var(--tw-ring-shadow, 0 0 #0000), var(--tw-shadow);
}
.buttom-circle:hover {
  --tw-scale-x: 1.05;
  --tw-scale-y: 1.05;
  transform: translate(var(--tw-translate-x), var(--tw-translate-y)) rotate(var(--tw-rotate)) skewX(var(--tw-skew-x)) skewY(var(--tw-skew-y)) scaleX(var(--tw-scale-x)) scaleY(var(--tw-scale-y));
}
.buttom-circle:active {
  --tw-scale-x: .95;
  --tw-scale-y: .95;
  transform: translate(var(--tw-translate-x), var(--tw-translate-y)) rotate(var(--tw-rotate)) skewX(var(--tw-skew-x)) skewY(var(--tw-skew-y)) scaleX(var(--tw-scale-x)) scaleY(var(--tw-scale-y));
}

/* Code copy button */
div.highlight,
pre {
  position: relative;
}

.codeblock-copy {
  position: absolute;
  right: 0.5rem;
  top: 0.5rem;
  margin-left: auto;
  margin-right: auto;
  display: flex;
  height: 1.75rem;
  width: 1.75rem;
  align-items: center;
  justify-content: center;
  border-radius: 0.375rem;
  border-width: 1px;
  border-color: transparent;
  transition-duration: 300ms;
}

.codeblock-copy:hover {
  --tw-border-opacity: 1;
  border-color: rgb(209 213 219 / var(--tw-border-opacity, 1));
}

.codeblock-copy:active {
  --tw-ring-offset-shadow: var(--tw-ring-inset) 0 0 0 var(--tw-ring-offset-width) var(--tw-ring-offset-color);
  --tw-ring-shadow: var(--tw-ring-inset) 0 0 0 calc(2px + var(--tw-ring-offset-width)) var(--tw-ring-color);
  box-shadow: var(--tw-ring-offset-shadow), var(--tw-ring-shadow), var(--tw-shadow, 0 0 #0000);
  --tw-ring-opacity: 1;
  --tw-ring-color: rgb(209 213 219 / var(--tw-ring-opacity, 1));
}

.codeblock-copy:hover:is(.dark *) {
  --tw-border-opacity: 1;
  border-color: rgb(55 65 81 / var(--tw-border-opacity, 1));
}

.codeblock-copy:active:is(.dark *) {
  --tw-ring-opacity: 1;
  --tw-ring-color: rgb(55 65 81 / var(--tw-ring-opacity, 1));
}

.icon-copy {
  height: 1.25rem;
  width: 1.25rem;
  fill: #d1d5db;
}

.icon-copy:is(.dark *) {
  fill: #6b7280;
}

.icon-copied {
  height: 1.25rem;
  width: 1.25rem;
}

/* Twikoo comment */
.twikoo {
  width: 100%;
  padding-left: 0.125rem;
  padding-right: 0.125rem;
  padding-top: 1.25rem;
  padding-bottom: 1.25rem;
}
.twikoo:is(.dark *) {
  --tw-text-opacity: 1;
  color: rgb(156 163 175 / var(--tw-text-opacity, 1));
}
@media (min-width: 768px) {

  .twikoo {
    padding-left: 0.5rem;
    padding-right: 0.5rem;
  }
}
@media (min-width: 1024px) {

  .twikoo {
    max-width: 64rem;
    padding-left: 2rem;
    padding-right: 2rem;
  }
}

.tk-submit .tk-row .tk-avatar {
  display: none;
}

.tk-row.actions {
  margin-left: 0px !important;
  margin-right: 0px !important;
}

.tk-input.el-textarea > textarea {
  height: 7rem !important;
  min-height: 7rem !important;
}

.tk-expand {
  border-radius: 0.5rem;
}

.tk-expand:hover:is(.dark *) {
  --tw-bg-opacity: 1;
  background-color: rgb(75 85 99 / var(--tw-bg-opacity, 1));
}

/* goods.html */
.goods-item-hidden {
  opacity: 0;
  transform: translateY(6%);
  transition:
    opacity 0.4s ease,
    transform 0.8s ease;
}

.slide-top {
  opacity: 1;
  transform: translateY(0);
}

:is(:where(.dark) .dark\:bg-gray-900\/80) {
  background-color: rgb(17 24 39 / 0.8);
}

/* flowbite-typography 引用语法修改 */
.format
  :where(blockquote p:first-of-type):not(
    :where([class~="not-format"] *)
  )::before {
  content: "" !important;
}

.format
  :where(blockquote p:last-of-type):not(
    :where([class~="not-format"] *)
  )::after {
  content: "" !important;
}

.format :where(blockquote):not(:where([class~="not-format"] *))::before {
  display: none !important;
  background-image: none !important;
}

.format
  :where(blockquote):not(
    :where([class~="not-format"], [class~="not-format"] *)
  ) {
  font-weight: 500;
  font-style: italic;
  color: var(--tw-format-quotes);
  border-left-width: 0.25rem;
  border-left-color: var(--tw-format-quote-borders);
  quotes: "\201C" "\201D" "\2018" "\2019";
  margin-top: 1.6em;
  margin-bottom: 1.6em;
  padding-left: 1em;
}

.format :where(a):not(:where([class~="not-format"] *)) {
  color: var(--tw-format-links);
  text-decoration: underline;
  text-decoration-thickness: 1px !important;
  text-underline-offset: 2px;
  font-weight: 500;
}

.format :where(a):not(:where([class~="not-format"] *)):hover {
  color: var(--hs-link-color);
  text-decoration: underline !important;
  text-decoration-color: var(--hs-decoration-color) !important;
  text-decoration-thickness: 2px !important;
}

