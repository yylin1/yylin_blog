  background: var(--hs-scrollbar-color);
  border-radius: 8px;
  cursor: pointer;
}

*::-webkit-scrollbar-thumb:hover {
  background: var(--hs-scrollbar-color-hover);
  opacity: 1;
  display: block !important;
}

/* 滚动条的轨道 */
