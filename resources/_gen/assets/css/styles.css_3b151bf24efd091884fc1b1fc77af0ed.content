.modal {
  position: fixed;
  left: 0px;
  top: 0px;
  z-index: 99;
  height: 100%;
  width: 100%;
  cursor: zoom-out;
  touch-action: none;
  user-select: none;
  background-color: rgb(255 255 255 / 0.75);
  --tw-backdrop-blur: blur(16px);
